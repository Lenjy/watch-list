<div class="banner" style="background-image: linear-gradient(rgba(0,0,0,0.4),rgba(0,0,0,0.4)), url(https://raw.githubusercontent.com/lewagon/fullstack-images/master/uikit/background.png);">
  <div class="container">
    <h1><%= @list.name %></h1>
    <%= link_to "add new bookmark",  new_list_bookmark_path(@list), class: "btn btn-primary" %>
  </div>
</div>
<div class="container">
  <div class="wrapper">
    <% @movies.each do |mov| %>
      <%= render 'cardmovie', movie: mov, list: @list %>
    <% end %>
  </div>
</div>
